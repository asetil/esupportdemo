<!DOCTYPE html>
<html lang="tr" class="no-js">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="WebMarket Admin Panel">
    <meta name="author" content="aware.co">

    <link href="~/resource/img/favico.ico" rel="shortcut icon" type="image/x-icon" />
    <title>@ViewBag.Title -ESupport Yönetim Paneli</title>

    @Styles.Render("~/resource/css")
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,300i,400,400i,500,500i,700,700i&amp;subset=latin-ext" rel="stylesheet">

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    @Scripts.Render("~/resource/js/common")
    @RenderSection("pageheader", required: false)
</head>
<body class="pd0 hold-transition skin-blue sidebar-mini fixed">
    <div class="wrapper">
        @Html.Partial("_SideBar")
        <div class="main-panel ps-container ps-theme-default ps-active-y">
            <div class="main-content">
                @RenderBody()
            </div>

            <footer class="footer">
                <div class="container-fluid">
                    <nav class="pull-left">
                        <ul>
                            <li><a href="#">Home</a> </li>
                            <li><a href="#">Company</a></li>
                            <li><a href="#">Portfolio </a></li>
                            <li><a href="#">Blog</a></li>
                        </ul>
                    </nav>
                    <p class="copyright pull-right">
                        © 2017 <a href="https://www.aware.com">Aware Inc.</a>, All Rights Reserved
                    </p>
                </div>
            </footer>
        </div>
    </div>
    @Html.Partial("_SiteModals")
</body>
</html>
